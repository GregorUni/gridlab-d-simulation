library(ggplot2)
library(scales)
library(stringr)
strom1 <- read.table(file = "strom1.csv",header=TRUE, sep = ",",stringsAsFactors = FALSE,comment.char = "", skip = 8)
strom1[1]<-substr(strom1[[1]],1,nchar(strom1[[1]])-4)
strom1[1]<-as.POSIXct(strom1[[1]], format='%Y-%m-%d  %H:%M:%S')
plot1<-ggplot(data=strom1, aes(x=strom1[[1]], y=strom1[[2]], group=1)) +geom_line()+ scale_x_datetime(labels = date_format("%Y-%m-%d  %H:%M:%S"))
